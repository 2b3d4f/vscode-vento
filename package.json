{
  "name": "vento-syntax",
  "displayName": "Vento Template Support",
  "description": "Vento language support",
  "version": "0.0.2",
  "publisher": "oscarotero",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.79.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oscarotero/vscode-vento"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "vento",
      "aliases": ["Vento", "vento"],
      "extensions": [".vto", ".vento"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "vento",
      "scopeName": "text.html.vento",
      "path": "./syntaxes/vento.tmLanguage.json"
    }],
    "snippets": [
      {
        "language": "vento",
        "scopename": "text.html.vento",
        "path": "./snippets/vento.json"
      }
    ]
  }
}
